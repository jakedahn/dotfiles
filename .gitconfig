[core]
  editor = vim
  excludesfile = ~/.gitignore
[user]
  name = jakedahn
  email = jake@ansolabs.com
[gitreview]
  username = jakedahn
[color]
  ui = auto
[color "branch"]
  current = blue reverse
  local = blue
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
